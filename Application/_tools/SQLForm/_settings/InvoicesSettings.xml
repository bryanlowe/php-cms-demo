<?xml version="1.0"?>
<form_config>
  <form title="Invoices">
    <input order="1" type="hidden" label="invoice_id" id="invoice_id" class="primaryKey"/>
    <input order="1" type="text" label="client_id" id="client_id"/>
    <input order="1" type="text" label="description" id="description"/>
    <input order="1" type="text" label="total_cost" id="total_cost"/>
    <input order="1" type="text" label="invoice_number" id="invoice_number"/>
    <input order="1" type="button" id="saveBtn" value="SAVE ENTRY" onclick="saveEntry('Invoices');"/>
    <input order="1" type="button" id="deleteBtn" value="DELETE ENTRY" onclick="deleteEntry('Invoices');"/>
  </form>
</form_config>

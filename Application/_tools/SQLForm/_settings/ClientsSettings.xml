<?xml version="1.0"?>
<form_config>
  <form title="Clients">
    <input order="1" type="hidden" label="client_id" id="client_id" class="primaryKey"/>
    <input order="1" type="text" label="company" id="company"/>
    <input order="1" type="text" label="client_name" id="client_name"/>
    <input order="1" type="text" label="email" id="email"/>
    <input order="1" type="text" label="phone_number" id="phone_number"/>
    <input order="1" type="text" label="client_rate" id="client_rate"/>
    <input order="1" type="button" id="saveBtn" value="SAVE ENTRY" onclick="saveEntry('Clients');"/>
    <input order="1" type="button" id="deleteBtn" value="DELETE ENTRY" onclick="deleteEntry('Clients');"/>
  </form>
</form_config>

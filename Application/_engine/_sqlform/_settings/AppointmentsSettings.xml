    <input order="1" type="hidden" label="appointment_id" id="appointment_id" class="primaryKey"/>
    <input order="1" type="text" label="client_id" id="client_id"/>
    <input order="1" type="text" label="appointment_date" id="appointment_date"/>
    <input order="1" type="text" label="description" id="description"/>
